                <xs:documentation>The quantity by which the order can be below the expected quantity.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="QuantityOver" type="DecimalElement5OverZeroType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>The quantity by which the order can be above the expected quantity.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="UnitOfMeasure" type="xs:string">
              <xs:annotation>
                <xs:documentation>The unit of measure.  (see Unit of Measure With Dollars list at dodprocurementtoolbox.com)</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <xs:attributeGroup ref="ChangeSection-OrderVariationQuantity"/>
        </xs:complexType>
