                      <xs:simpleContent>
                        <xs:extension base="TrueType">
                          <xs:attributeGroup ref="ChangeDetails-AddDelete"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ForeignCurrency" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>The textual name of the foreign currency that must be used in the response (may include US Dollar from ISO 4217 from the International Organization for Standardization.  (see Currency list at dodprocurementtoolbox.com)</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attributeGroup ref="ChangeDetails-AddDelete"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="TaxesModificationDetails" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Changes to taxes that apply to the total order amount.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AddedTaxes" type="TaxDetailType1" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains taxes that are added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedTaxes" type="TaxDetailType1" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains taxes that are removed.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedTaxes" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains taxes that are modified.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="TaxesChangeText" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Description of the change to the tax.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CurrentTax" type="TaxDetailType1">
                    <xs:annotation>
                      <xs:documentation>The tax after the change was applied.  The result.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PreviousTax" type="TaxDetailType1">
                    <xs:annotation>
                      <xs:documentation>The tax before the change was applied.  The original.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ObligatedAmountsModificationDetails" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Changes to obligation amounts.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AddedObligatedAmounts" type="ContractObligatedAmountType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains obligated amounts that are added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedObligatedAmounts" type="ContractObligatedAmountType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Used when obligated amount is reduced to zero because of the modification.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedObligatedAmounts" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Used to record changes to an existing obligated amount and/or associated information that does not result in a zero obligated amount.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="ObligatedAmountsChangeText" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Description of the change to the obligated amount.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="ObligatedAmountDelta" type="DecimalElement5Type">
                    <xs:annotation>
                      <xs:documentation>The change in the obligated amount.  If no change, enter zero.  If the current obligated amount is less than the previous obligated amount, enter a negative number.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CurrentObligatedAmount" type="ContractObligatedAmountType">
                    <xs:annotation>
                      <xs:documentation>The obligated amount after the change was applied.  The result.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PreviousObligatedAmount" type="ContractObligatedAmountType">
                    <xs:annotation>
                      <xs:documentation>The obligated amount before the change was applied.  The original.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="OtherAmounts" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Amounts other than obligated amounts.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="AmountType">
              <xs:attributeGroup ref="ChangeSection-OtherAmounts"/>
            </xs:extension>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="UndefinitizedOtherAmountsModificationDetails" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Changes to undefinitized other amounts.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AddedUndefinitizedOtherAmounts" type="UndefinitizedOtherAmountsType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Contains undefinitized other amounts that are added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedUndefinitizedOtherAmounts" type="UndefinitizedOtherAmountsType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Used when undefinitized other amounts are reduced to zero because of the modification.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedUndefinitizedOtherAmounts" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Used to record changes to an existing undefinitized other amounts that does not result in a zero undefinitized other amount.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="UndefinitizedOtherAmountsChangeText" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Description of the change to the undefinitized other amounts.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CurrentUndefinitizedOtherAmounts" type="UndefinitizedOtherAmountsType">
                    <xs:annotation>
                      <xs:documentation>The undefinitized other amounts after the change was applied.  The result.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PreviousUndefinitizedOtherAmounts" type="UndefinitizedOtherAmountsType">
                    <xs:annotation>
                      <xs:documentation>The undefinitized other amounts before the change was applied.  The original.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ProcurementInstrumentHeaderModificationDetails">
    <xs:annotation>
      <xs:documentation>Data common to the entire award modification or solicitation amendment procurement instrument.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ProcurementInstrumentIdentifier" type="ProcurementInstrumentType">
        <xs:annotation>
          <xs:documentation>The unique identifier of the modified or amended procurement instrument.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProcurementInstrumentIdentifierModificationDetails" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
