  <xs:complexType name="ClauseDetailsType">
    <xs:sequence>
      <xs:choice>
        <xs:element name="RegulationOrSupplement" type="ClauseRegulationSupplementType">
          <xs:annotation>
            <xs:documentation>Regulation or Supplement name such as FAR, DFARS and AFARS. (see list)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="IssuingActivity" type="xs:string">
          <xs:annotation>
