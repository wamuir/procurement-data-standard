                  <xs:documentation>Quantity based range or threshold.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="AmountRange" type="QtyAmountRangeType">
                <xs:annotation>
                  <xs:documentation>Amount based range or threshold.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:choice>
            <xs:element name="SACValue" type="DiscountValueType1">
              <xs:annotation>
                <xs:documentation>The value and type of the service, allowance, or charge. </xs:documentation>
              </xs:annotation>
