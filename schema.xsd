                  <xs:enumeration value="Gigahertz"/>
                  <xs:enumeration value="Hertz"/>
                  <xs:enumeration value="Kilobaud"/>
                  <xs:enumeration value="Kilobits Per Second"/>
                  <xs:enumeration value="Kilohertz"/>
                  <xs:enumeration value="Megabaud"/>
                  <xs:enumeration value="Megabits Per Second"/>
                  <xs:enumeration value="Megahertz"/>
                  <xs:enumeration value="Terabaud"/>
                  <xs:enumeration value="Terabit"/>
                  <xs:enumeration value="Terabits Per Second"/>
                  <xs:enumeration value="Terahertz"/>
