          <xs:sequence>
            <xs:element name="AddedConstructionProject" type="ConstructionProjectDetailsType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains construction project information that is added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedConstructionProject" type="ConstructionProjectDetailsType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains construction project information that is deleted.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedConstructionProject" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains construction project information that is modified.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="ConstructionProjectChangeText" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Description of the change to the construction project information.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="CurrentConstructionProject" type="ConstructionProjectDetailsType">
