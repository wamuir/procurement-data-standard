                                    <xs:enumeration value="Skid"/>
                                    <xs:enumeration value="Skid, box"/>
                                    <xs:enumeration value="Spool"/>
                                    <xs:enumeration value="Suitcase"/>
                                    <xs:enumeration value="Tank Car"/>
                                    <xs:enumeration value="Tank Truck"/>
                                    <xs:enumeration value="Truck"/>
                                    <xs:enumeration value="Tub"/>
                                    <xs:enumeration value="Tube"/>
                                    <xs:enumeration value="Unitized"/>
                                    <xs:enumeration value="Van Chassis"/>
                                    <xs:enumeration value="Vehicle"/>
                                    <xs:enumeration value="Vehicle in operating condition"/>
                                    <xs:enumeration value="Wrapped"/>
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:element>
                              <xs:element name="PackingCapacity" minOccurs="0">
                                <xs:annotation>
                                  <xs:documentation>To what level the container will be packed.  May only be provided when Packing Container is "MILVAN", "SEAVAN", or "SDDC leased/controlled SEAVAN or MILVAN (SDDCVAN)".  (see list)</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                  <xs:restriction base="xs:token">
                                    <xs:enumeration value="Empty"/>
                                    <xs:enumeration value="Less Than Capacity"/>
                                    <xs:enumeration value="To Capacity"/>
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:element>
                              <xs:element name="Packer" minOccurs="0">
                                <xs:annotation>
                                  <xs:documentation>Specifies who will pack the container.  Not provided when Packing Capacity is "Empty".  (see list)</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                  <xs:restriction base="xs:token">
                                    <xs:enumeration value="Contract Shipment Consolidation Facility"/>
                                    <xs:enumeration value="Military Shipping Activity"/>
                                    <xs:enumeration value="Military Terminal"/>
                                    <xs:enumeration value="Ocean Carrier"/>
                                    <xs:enumeration value="Vendor"/>
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:element>
                              <xs:element name="AdditionalPacking" minOccurs="0">
                                <xs:annotation>
                                  <xs:documentation>Use when a second packer is expected.  May only be provided when Packing Capacity is "Less Than Capacity"</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                  <xs:choice>
                                    <xs:sequence>
                                      <xs:element name="AdditionalPackingCapacity">
                                        <xs:annotation>
                                          <xs:documentation>To what level the container will be packed by the second packer.  (see list)</xs:documentation>
                                        </xs:annotation>
                                        <xs:simpleType>
                                          <xs:restriction base="xs:token">
                                            <xs:enumeration value="Completed"/>
                                            <xs:enumeration value="To Capacity"/>
                                          </xs:restriction>
                                        </xs:simpleType>
                                      </xs:element>
                                      <xs:element name="AdditionalPacker">
                                        <xs:annotation>
                                          <xs:documentation>Specifies who will pack the container.  (see list)</xs:documentation>
                                        </xs:annotation>
                                        <xs:simpleType>
                                          <xs:restriction base="xs:token">
                                            <xs:enumeration value="Contract Shipment Consolidation Facility"/>
                                            <xs:enumeration value="Military Shipping Activity"/>
                                            <xs:enumeration value="Military Terminal"/>
                                            <xs:enumeration value="Ocean Carrier"/>
                                            <xs:enumeration value="Vendor"/>
                                          </xs:restriction>
                                        </xs:simpleType>
                                      </xs:element>
                                    </xs:sequence>
                                    <xs:element name="CommingledPacking" type="TrueType">
                                      <xs:annotation>
                                        <xs:documentation>Commingled with commercial cargo in accordance with the SDDC Container Agreement and Rate Guide.  Only used when Packer is "Ocean Carrier".  (see list)</xs:documentation>
                                      </xs:annotation>
                                    </xs:element>
                                  </xs:choice>
                                </xs:complexType>
                              </xs:element>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                        <xs:element name="ProtectionLevel" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Indicates the packaging protection level (preservation) that a price is to be provided for.  May only be provided if Other Pricing Description is Packaging Protection Level (Preservation) Price.</xs:documentation>
                          </xs:annotation>
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="PreservationMethod">
                                <xs:annotation>
                                  <xs:documentation>Method and sub-method of preservation (MIL-STD-2073-1D, appendix J).  (see list)</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                  <xs:restriction base="xs:token">
                                    <xs:enumeration value="Physical Protection (10)"/>
                                    <xs:enumeration value="Preservative Coating Only (20)"/>
                                    <xs:enumeration value="Waterproof or Waterproof-Greaseproof Protection (30)"/>
                                    <xs:enumeration value="Waterproof Bag, Sealed (31)"/>
                                    <xs:enumeration value="Container, Waterproof Bag, Sealed (32)"/>
                                    <xs:enumeration value="Greaseproof-Waterproof Bag, Sealed (33)"/>
                                    <xs:enumeration value="Watervaporproof Protection (40)"/>
                                    <xs:enumeration value="Watervaporproof Bag, Sealed (41)"/>
                                    <xs:enumeration value="Container, Watervaporproof Bag, Sealed (42)"/>
                                    <xs:enumeration value="Floating Waterproof Bag, Sealed (43)"/>
                                    <xs:enumeration value="Rigid Container (Other Than Metal), Sealed (44)"/>
                                    <xs:enumeration value="Rigid Metal Container, Sealed (45)"/>
                                    <xs:enumeration value="Watervaporproof Protection With Desiccant (50)"/>
                                    <xs:enumeration value="Watervaporproof Bag, Sealed (51)"/>
                                    <xs:enumeration value="Container, Watervaporproof Bag, Sealed (52)"/>
                                    <xs:enumeration value="Floating Waterproof Bag, Sealed (53)"/>
                                    <xs:enumeration value="Rigid Container (Other Than Metal), Sealed (54)"/>
                                    <xs:enumeration value="Rigid Metal Container, Sealed (55)"/>
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:element>
                              <xs:element name="SpecialPreservationCode" minOccurs="0" maxOccurs="unbounded">
                                <xs:annotation>
                                  <xs:documentation>Specialized preservation codes (MIL_STD_2073-1D J.4.2.1).</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                  <xs:restriction base="xs:token">
                                    <xs:enumeration value="AE"/>
                                    <xs:enumeration value="AH"/>
                                    <xs:enumeration value="AU"/>
                                    <xs:enumeration value="AW"/>
                                    <xs:enumeration value="BC"/>
                                    <xs:enumeration value="BD"/>
                                    <xs:enumeration value="BL"/>
                                    <xs:enumeration value="DB"/>
                                    <xs:enumeration value="DC"/>
                                    <xs:enumeration value="DR"/>
                                    <xs:enumeration value="DW"/>
                                    <xs:enumeration value="EK"/>
                                    <xs:enumeration value="GS"/>
                                    <xs:enumeration value="GX"/>
                                    <xs:enumeration value="HM"/>
                                    <xs:enumeration value="JF"/>
                                    <xs:enumeration value="JM"/>
                                    <xs:enumeration value="KD"/>
                                    <xs:enumeration value="KF"/>
                                    <xs:enumeration value="KG"/>
                                    <xs:enumeration value="KH"/>
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:element>
                              <xs:element name="SupplementaryOrSpecialRequirements" type="xs:string" minOccurs="0">
                                <xs:annotation>
                                  <xs:documentation>Supplementary or Special Requirements.  (MIL_STD_2073-1D J.1.4.c)</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                        <xs:element name="ShippingPostalCode" type="PostalCodeType" minOccurs="0" maxOccurs="unbounded">
                          <xs:annotation>
                            <xs:documentation>The general shipping area for quoting purposes.   When Other Pricing Description is "Zone Price", either Delivery Zone or Shipping Postal Code is required; both may be provided.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="QuantityRangePricing" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Denotes the quantity ranges when prices are being solicited for step ladder pricing.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="QuantityRange" maxOccurs="unbounded">
                          <xs:annotation>
                            <xs:documentation>The quantity range for which a price is being solicited.</xs:documentation>
                          </xs:annotation>
                          <xs:complexType mixed="false">
                            <xs:complexContent>
                              <xs:extension base="QtyAmountRangeType"/>
                            </xs:complexContent>
                          </xs:complexType>
                        </xs:element>
                        <xs:element name="UnitOfMeasure" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>The unit of measure of the quantity range.  (see Unit of Measure Without Dollars list at dodprocurementtoolbox.com)</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="SetAsideDetails" type="SetAsideDetailsType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Provides set aside percentage and organization type.  If not provided, the solicitation is unrestricted.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="OrderingInformation" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Ordering information pertinent to the line item.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="EstimatedQuantity" type="EstimatedQuantityType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>The estimated total quantity to be ordered. The actual order quantity may be higher or lower.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Limits" type="LimitsType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Ordering limits on the line item.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="OrderVariation" type="OrderVariationType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Permitted variation in quantity.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="PricingArrangement" type="PricingArrangementType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Pricing arrangement type applicable to the line item.  (FAR Part 16)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProductServicesOrdered" type="ProductServiceQuantityType">
        <xs:annotation>
          <xs:documentation>Details of the product or services ordered.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LineItemDatesType">
    <xs:annotation>
      <xs:documentation>Date structure for a line item.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="LineItemDateDescription">
        <xs:annotation>
          <xs:documentation>Description of the date.  (see list)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Funding Period"/>
            <xs:enumeration value="Lease"/>
            <xs:enumeration value="Line Item Effective Date"/>
            <xs:enumeration value="Option Exercise Date"/>
            <xs:enumeration value="Ordering Period"/>
            <xs:enumeration value="Period of Performance"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:choice>
        <xs:element name="LineItemDate" type="DateTimeType">
          <xs:annotation>
            <xs:documentation>A specific date (and time if needed).</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="LineItemMonth">
          <xs:annotation>
            <xs:documentation>Date in CCYY-MM format.</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:gYearMonth"/>
          </xs:simpleType>
        </xs:element>
        <xs:element name="LineItemPeriod" type="DatePeriodType">
          <xs:annotation>
            <xs:documentation>Denotes a range of dates.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LineItemDetailType">
    <xs:annotation>
      <xs:documentation>Top Level Line Item Details</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="LineItemIdentifier" type="LineItemIdentifierType">
        <xs:annotation>
          <xs:documentation>Number of the line item.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LineItemBasicInformation" type="LineItemBasicInformationType">
        <xs:annotation>
          <xs:documentation>Miscellaneous information about the line item.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Addresses" type="AddressDetailType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Addresses applicable to the line item.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Attachments" type="AttachmentType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Attachments applicable to the line item.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Delivery" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Line item deliveries and ship-to information. </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ExcessDeliveryAction" type="ExcessDeliveryActionType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Action to be taken when the maximum total order amount is exceeded by the contractor.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ItemDeliveryDates" type="DeliveryDatesType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Date or date ranges that relate to a delivery.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ItemSchedule" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Information on deliveries that require a lead time.</xs:documentation>
              </xs:annotation>
              <xs:complexType mixed="false">
                <xs:complexContent>
                  <xs:extension base="ItemDeliveryLeadTimeScheduleType"/>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="ItemSpecialHandling" type="DeliverySpecialHandlingHazmatType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Special handling and hazardous material details.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="PartialDeliverySchedule" type="ItemPartialDeliveryType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Partial delivery details.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="PreDeliveryNotification" type="TrueFalseType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Indicates consignee or a shipping contact must be notified to schedule a delivery.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ShipToAddress" type="AddressDetailType" minOccurs="0" maxOccurs="2">
              <xs:annotation>
                <xs:documentation>Address where the delivery will be made. One iteration where Address Description is 'Service Performance Site', 'Ship To', 'Shipment in Place Location', or 'Place of Performance' must be provided.  A second iteration where Address Description is 'Mark for Party' may be provided.  Must be provided on an award or modification when the line item is not an option line item.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="TransportationDetails" type="RoutingTransitType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Transportation information</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Discounts" type="DiscountDetailType1" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Discounts applicable to the line item.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LineItemClauses" type="ClauseDetailsType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Clause or provision information that applies to the line item.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LineItemAmounts" type="LineItemAmountsType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Amounts and pricing that impact the line item.  Do not provide on a Solicitation.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LineItemDates" type="LineItemDatesType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Dates applicable to the line item that are not related to a delivery. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PackPhysicalDetails" type="ItemPhysicalDetailsType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Physical quantities with relation to a pack.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PriceDetails" type="UnitPriceType1" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Line item pricing information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RecurringPayments" type="InstallmentType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Recurring payments of a fixed amount that occur on a specified schedule such as rent.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ReferenceNumber" type="ItemReferenceNumberType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Use to provide numbers referencing documents (MIPR, SOW, GBL, Requisition etc.). Numbers (FMS Case, Package, Drawing etc.), Codes (SFC, Project, Weapon System, etc.) and other Reference Values.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RequirementsDescription" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Uniform Contract Format (UCF) Section C.  Any description or specifications needed in addition to UCF Section B.  (This carries statements of objectives, performance work statements, statements of work and similar text descriptions when not separated in an attachment.  When such documents are attachments, this cites the portions relevant to the cited line item.  Specific numbered specifications, standards, and other standardized descriptive data is handled separately under Product or Service Identifiers and Reference Numbers)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ServiceAllowanceCharges" type="ServiceAllowanceChargeType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Services, Allowances and Charges.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Shipping" type="ShippingType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Information pertaining to shipments.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TelecommunicationRates" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Details on telecommunication rates.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="RateDescription" type="xs:string">
              <xs:annotation>
                <xs:documentation>Brief description about what the rate applies to.  For example, MES to Cellular Networks, SMS Messaging, etc.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="Rate" type="DecimalElement5OverZeroType">
              <xs:annotation>
                <xs:documentation>The rate amount.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="RateUnitOfMeasure" type="xs:string">
              <xs:annotation>
                <xs:documentation>The unit of measure to which the rate applies such as second, messages, etc.  (see Unit of Measure Without Dollars list at dodprocurementtoolbox.com)</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="WageDeterminationDetails" type="WageDeterminationType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Wage determination information.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LineItemIdentifierType">
    <xs:annotation>
      <xs:documentation>Line item type and numbering.</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="DFARS" type="DFARSLineItemType">
        <xs:annotation>
          <xs:documentation>Line item number as defined in the Department of Defense Federal Acquisition Regulation Supplement (DFARS).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FAR" type="FARLineItemType">
        <xs:annotation>
          <xs:documentation>Line item number as defined in the Federal Acquisition Regulation (FAR).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="LineItemsType">
    <xs:sequence>
      <xs:element name="LineItemType">
        <xs:annotation>
          <xs:documentation>The type of line item. (see list)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="CLIN"/>
            <xs:enumeration value="SLIN (Separately Identified)"/>
            <xs:enumeration value="INFOSLIN"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="LineItemBase" type="LineItemBaseType">
        <xs:annotation>
          <xs:documentation>The first four numbers of the line item. (Range 0001 to 9999) </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="LineItemExtension" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Extension added to the line item base.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="InfoSlinExtension">
              <xs:annotation>
                <xs:documentation>Two digit extension denoting an information sub line item. (Range 01-99)</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:pattern value="([0]){1}([1-9]){1}"/>
                  <xs:pattern value="([1-9]){1}([0-9]){1}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="SlinExtension" type="SlinExtensionType">
              <xs:annotation>
                <xs:documentation>Two character extension denoting a sub line item. (Range AA-ZZ, except letters I or O).</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LoaCodeType">
    <xs:sequence>
      <xs:element name="ComponentTitle" type="xs:string">
        <xs:annotation>
          <xs:documentation>Component description.  (see LOA Component list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ComponentValue" type="xs:string">
        <xs:annotation>
          <xs:documentation>The value of the component listed in Component Title.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LoaDetailsType">
    <xs:annotation>
      <xs:documentation> Without originating organization as a separate element.</xs:documentation>
    </xs:annotation>
  </xs:complexType>
  <xs:complexType name="LocationCoordinatesType">
    <xs:annotation>
      <xs:documentation>The longitude and latitude coordinates of a location.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Latitude" type="DecimalElementCoordinateType">
        <xs:annotation>
          <xs:documentation>The location North or South of the Equator.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Longitude" type="DecimalElementCoordinateType">
        <xs:annotation>
          <xs:documentation>The location East or West of the Prime Meridian.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MarkLabelType">
    <xs:annotation>
      <xs:documentation>Identifies the label that should be contained with the mark</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="MarkValue" type="xs:string">
        <xs:annotation>
          <xs:documentation>The text of the mark.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MarkLabel" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>When provided, this is the label that identifies the marking and is part of the marking.  For example, "Serial Number:", "Manufacturer Part Number -", etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MarkRangeType">
    <xs:sequence>
      <xs:element name="RangeStart" type="xs:string">
        <xs:annotation>
          <xs:documentation>Starting number of the sequential range</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RangeEnd" type="xs:string">
        <xs:annotation>
          <xs:documentation>Ending number of the sequential range</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MarkLabel" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>When provided, this is the label that identifies the marking and is part of the marking.  For example, "Serial Number:", "Manufacturer Part Number -", etc.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MiscellaneousTextDetailsType">
    <xs:annotation>
      <xs:documentation>Free form text and the UCF section it belongs to.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="MiscellaneousText" type="xs:string">
          <xs:annotation>
            <xs:documentation>Text associated with a particular section from the Federal Uniform Contract Format (UCF) or CSI Division or Section.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="FormattedMiscellaneousText" type="xs:string">
          <xs:annotation>
            <xs:documentation>Text associated with a particular section from the Federal Uniform Contract Format (UCF) or CSI Division or Section with html tags.  Contents must be enclosed with CDATA.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
      <xs:group ref="ProcurementInstrumentLocationGroup">
        <xs:annotation>
          <xs:documentation>Provides the procurement instrument location that the information is associated with.</xs:documentation>
        </xs:annotation>
      </xs:group>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OrganizationIDType">
    <xs:annotation>
      <xs:documentation>Organization ID as pre-defined by various entities.  Examples are DUNS, CAGE, DoDAAC etc. 
In accordance with DoD BTA BEA 4.1 OV-7 ORGANIZATION_IDENTIFIER entity.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AustinTetraNumber" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Austin-Tetra Universal Supplier Identification Number (A-T Number).</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9]){10}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Cage" type="CAGEType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>CAGE: Commercial and Government Entity  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CountryCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Three character alpha Country code from Geopolitical Entities, Names, and Codes (GENC).  (see Country list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([A-Z]){3}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="DemanderCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation>A six digit numeric code denoting the Service Ordering Authority on a telecommunications document.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9]){6}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="DoDAAC" type="DoDAACType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>DODAAC: Department of Defense Activity Address Code. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Duns4Number" type="Duns4NumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>DUNS+4: Data Universal Numbering System identifier with four character suffix.  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DunsNumber" type="DunsNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>DUNS:  Data Universal Numbering System Identifier.  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EFTIndicator" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The Electronic Funds Transfer (EFT) indicator.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9A-Z]){4}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="GovernmentUniqueEntityIdentifier" type="GovernmentUniqueEntityIdentifierType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Government Unique Entity Identifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Mapac" type="MAPACType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>MAPAC: Military Assistance Program Address Code.  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OUID" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>OUID:  Organization Unique Identifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SocialSecurityNumber" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Number obtained from the Social Security Administration (SSA) that identifies an individual.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9]){9}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="EmployerIdentificationNumber" minOccurs="0">
        <xs:annotation>
          <xs:documentation>An Employer Identification Number (EIN) issued by the IRS and used to identify a business entity.  Also known as a federal tax identification number.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9]){9}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Zone" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Delivery Zones: Values include "CONUS", "WORLD".  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ShippingPostalCode" type="PostalCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The designator that distinguishes one postal zone from another.  May only be used on a solicitation when Address Description is Ship To.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AlternateIdentifier" type="AlternateIdentifierType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Use to provide other identifiers that aren't provided as distinct elements.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OrganizationAddressType">
    <xs:choice>
      <xs:element name="FreeFormAddress" type="FreeFormAddressType">
        <xs:annotation>
          <xs:documentation>The address in free form text.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RealPropertyAddress" type="RealPropertyAddressType">
        <xs:annotation>
          <xs:documentation>Address structure for real property as defined by the DoD Business Enterprise Architecture (BEA).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="USPostalAddress" type="USPostalAddressType"/>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="OrganizationDetailType">
    <xs:annotation>
      <xs:documentation>Generic Organization Details - Common for supplier, buyer, contractor etc.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="OrganizationID" type="OrganizationIDType">
        <xs:annotation>
          <xs:documentation>Organization Identifier.  When Address Description is "Survey Site" or "Pre-Property Inspection Location" and no other Organization Identifier applies, provide Zone or Alternate Identifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OrganizationNameAddress" type="OrganizationNameAddressType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Required if the organization cannot be identified in the "OrganizationId" with an acceptable alternative.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OrganizationNameAddressType">
    <xs:annotation>
      <xs:documentation>Organization Name and Address generic type. </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="OrganizationName" type="xs:string">
        <xs:annotation>
          <xs:documentation>The name of the organization.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OrganizationAddress" type="OrganizationAddressType">
        <xs:annotation>
          <xs:documentation>The address of the organization.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OrganizationCoordinates" type="LocationCoordinatesType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The longitude and latitude coordinates of the address. </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OtherDiscountsType">
    <xs:sequence>
      <xs:element name="OtherDiscountDescription">
        <xs:annotation>
          <xs:documentation>Type of general discount.  (see list)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="General Discount"/>
            <xs:enumeration value="Credit Card Payment Discount"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Discount" type="DiscountValueType2">
        <xs:annotation>
          <xs:documentation>The discount to be applied.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PackagingMarkingLoadingType">
    <xs:annotation>
      <xs:documentation> High level Packaging requirements.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="PackagingDescription" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Describes a packaging requirement. (see list)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Cleaning or Drying Procedure"/>
            <xs:enumeration value="Cushioning Thickness"/>
            <xs:enumeration value="Cushioning and Dunnage"/>
            <xs:enumeration value="Intermediate Container"/>
            <xs:enumeration value="Optional Procedure Indicator"/>
            <xs:enumeration value="Commercial Packaging"/>
            <xs:enumeration value="Preservation Method"/>
            <xs:enumeration value="Preservation Material"/>
            <xs:enumeration value="Special Marking"/>
            <xs:enumeration value="Unit Container"/>
            <xs:enumeration value="Unit Container Level"/>
            <xs:enumeration value="Wrapping Material"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="PackagingSpecifications" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Packaging details.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="AgencySpecifiedPackaging" type="AgencySpecifiedPackagingType" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Packaging characteristics from an authoritative source. </xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="SpecialInstructions" type="xs:string" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Use to provide supplemental or special packaging instructions.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="PackagingLoadOption" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Shipment options of palletized or shrink-wrap. (see list)</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:token">
                  <xs:enumeration value="Pallet"/>
                  <xs:enumeration value="Pallet - 2 Way"/>
                  <xs:enumeration value="Pallet - 4 Way"/>
                  <xs:enumeration value="Pallet Wingtip"/>
                  <xs:enumeration value="Shrink-wrap"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PackingQuantityType">
    <xs:sequence>
      <xs:element name="PackQuantity" type="xs:positiveInteger">
        <xs:annotation>
          <xs:documentation>Number of units (pieces, supplier units, unit packs etc.) in an individual pack.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SpecialCondition">
        <xs:annotation>
          <xs:documentation>Special condition applicable to the pack quantity. (see list)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Bulk"/>
            <xs:enumeration value="Packager's Option"/>
            <xs:enumeration value="Special Requirement"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PackMeasurementsType">
    <xs:sequence>
      <xs:element name="Weight" type="WeightType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The pack weight.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Volume" type="VolumeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The pack volume.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Size" type="SizeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The pack size.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ParentClinType">
    <xs:sequence>
      <xs:element name="LineItemBase" type="LineItemBaseType">
        <xs:annotation>
          <xs:documentation>The first four numbers of the line item. (Range 0001 to 9999)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SlinExtension" type="SlinExtensionType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Two character extension denoting a sub line item. (Range AA-ZZ, except letters I or O).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PaymentDiscountType">
    <xs:sequence>
      <xs:element name="Terms" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Terms of the discount. </xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Percent" type="DecimalElement5OverZeroType">
              <xs:annotation>
                <xs:documentation>Discount percentage.  Express 4.25% as 4.25</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DaysDue" type="xs:positiveInteger">
              <xs:annotation>
                <xs:documentation>Number of calendar days by which payment must be made to receive the discount.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="NetDays" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Number of days until total invoice amount is due.  Must be between 1 and 30 inclusive.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:positiveInteger">
            <xs:maxInclusive value="30"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PercentAmountType">
    <xs:sequence>
      <xs:element name="PercentDescription">
        <xs:annotation>
          <xs:documentation>The type of percentage to be applied. (see list)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Profit or Fee-Cost"/>
            <xs:enumeration value="Profit or Fee-Hours"/>
            <xs:enumeration value="Escalation Factors"/>
            <xs:enumeration value="Progress Payment"/>
            <xs:enumeration value="Progress Payment Liquidation"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="PercentValue" type="DecimalElement5Type">
        <xs:annotation>
          <xs:documentation>The percentage to be applied. Express 4.25% as 4.25</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PostalCodeType">
    <xs:choice>
      <xs:sequence>
        <xs:element name="USZipCode">
          <xs:annotation>
            <xs:documentation>5 digit postal code used in the United States.</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:pattern value="([0-9]){5}"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="USZipCodeExtension" minOccurs="0">
          <xs:annotation>
            <xs:documentation>4 digit postal code extension used in the United States.</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:pattern value="([0-9]){4}"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
      <xs:element name="ForeignPostalCode">
        <xs:annotation>
          <xs:documentation>Postal zone designator for non-US addresses.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="ProcurementInstrumentHeaderType">
    <xs:sequence>
      <xs:element name="ProcurementInstrumentIdentifier" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Unique Identifier for a procurement instrument.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:complexContent>
            <xs:extension base="ProcurementInstrumentType"/>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="BasicInformation" type="HeaderBasicInformationType">
        <xs:annotation>
          <xs:documentation>Miscellaneous procurement instrument information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ClauseInformation" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Clause or provision information that applies to the procurement instrument.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="RegulationURL" type="xs:string" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>The URL where the text of the clauses or provisions in a Regulation, Supplement, or Issuing Agency can be found.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ClauseDetails" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Information about a clause or provision. </xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="ClauseDetailsType"/>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ConstructionProjectDetails" type="ConstructionProjectDetailsType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Information about one or more projects in a construction contract.  May only be used when Procurement Instrument Form is SF 1442 or SF 252</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProcurementInstrumentDates" type="DatesHeaderOnlyType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Dates and date period applicable to the procurement instrument.  Required on Awards.  Only Procurement Instrument Periods is allowed on solicitations.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProcurementInstrumentAddresses" type="AddressDetailType" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Addresses applicable to the procurement instrument.  Two addresses, Issuing Office and Contractor, must be provided on awards.  Issuing Office must be provided on solicitations.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProcurementInstrumentAmounts" type="ProcurementInstrumentAmountsType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Amounts and pricing that impact the entire procurement instrument.  On Solicitations, use Header Currency only.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DeliveryDetails" type="DeliveryDetailsType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Delivery details including dates that impact the entire procurement instrument. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MultipleLineItemsInformation" type="MultipleLineItemsInformationType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Provides selected information such as clauses, discounts, etc. that is applicable to a subset of the procurement instrument line items.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OrderingDiscounts" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Discounts applicable to the award instrument.</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="false">
          <xs:complexContent>
            <xs:extension base="DiscountDetailType1"/>
          </xs:complexContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="PaymentDiscount" type="PaymentDiscountType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Payment discount information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProcurementInstrumentAttachments" type="AttachmentType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Identifies paperwork that is required to be attached to the procurement instrument.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RecurringPayments" type="InstallmentType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Recurring payments of a fixed amount that occur on a specified schedule such as rent.  Do not provide on a Solicitation.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ReferenceNumber" type="ReferenceNumberType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Use to provide numbers referencing documents (MIPR, SOW, GBL, Requisition etc.). Numbers (FMS Case, Package, Drawing etc.), Codes (FSC, Project, Weapon System, etc.) and other Reference Values.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ServiceAllowanceCharges" type="ServiceAllowanceChargeType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Services, Allowances and Charges.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Shipping" type="ShippingType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Information pertaining to shipments.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RequirementsDescription" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Uniform Contract Format (UCF) Section C.  Any description or specifications needed in addition to UCF Section B.  (This carries statements of objectives, performance work statements, statements of work and similar text descriptions when not separated in an attachment.  Specific numbered specifications, standards, and other standardized descriptive data is handled separately under Product or Service Identifiers and Reference Numbers)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TelecommunicationDetails" type="TelecommunicationDetailsType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Information relative to a telecommunications contract.  May only be used when Procurement Instrument Form is DD 428.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="WageDeterminationDetails" type="WageDeterminationType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Wage determination information.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ProcurementInstrumentNumberFormatType">
    <xs:sequence>
      <xs:element name="EnterpriseIdentifier">
        <xs:annotation>
          <xs:documentation>A generic code that identifies the entity that issued the procurement instrument.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:minLength value="6"/>
            <xs:maxLength value="6"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Year">
        <xs:annotation>
          <xs:documentation>Code that identifies the year</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([0-9]){2}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ProcurementInstrumentTypeCode" type="ProcurementInstrumentTypeCodeType">
        <xs:annotation>
          <xs:documentation>One digit code that identifies the type of procurement instrument.  (see list)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SerializedIdentifier">
        <xs:annotation>
          <xs:documentation>Value that separates this procurement instrument from all other procurement instruments with the same Enterprise Identifier, Year, and Procurement Instrument Type Code.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="([1-9A-HJ-NP-Z]){1}([0-9A-HJ-NP-Z]){3}"/>
            <xs:pattern value="([0-9A-HJ-NP-Z]){1}([1-9A-HJ-NP-Z]){1}([0-9A-HJ-NP-Z]){2}"/>
            <xs:pattern value="([0-9A-HJ-NP-Z]){2}([1-9A-HJ-NP-Z]){1}([0-9A-HJ-NP-Z]){1}"/>
            <xs:pattern value="([0-9A-HJ-NP-Z]){3}([1-9A-HJ-NP-Z]){1}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="OrderNumber" type="OrderNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The release, delivery, or task order number.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OrderModificationIdentifier" type="OrderModificationIdentifierType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Value that identifies the modification number of the order.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ProcurementInstrumentModificationIdentifier" type="ProcurementInstrumentModificationIdentifierType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Value that identifies the modification number of the procurement instrument.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AmendmentNumber" type="AmendmentNumberType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The solicitation amendment number.  Must be 4 numeric digits between 0001 and 9999.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PricingArrangementType">
    <xs:sequence>
      <xs:element name="PricingArrangementBase" type="PricingArrangementBaseType">
