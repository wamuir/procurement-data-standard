          <xs:documentation>May only provide multiple iterations at this location if Product Service Group is provided.</xs:documentation>
        </xs:annotation>
        <xs:element name="DataDescription" type="xs:string">
          <xs:annotation>
            <xs:documentation>Description of the Data Value contents. (see Product Service Data list at dodprocurementtoolbox.com)</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="DataValue" type="xs:string">
          <xs:annotation>
            <xs:documentation>The value associated with the product or service. </xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ProductServiceQuantityType">
    <xs:annotation>
      <xs:documentation> Product/Service and the quantity being purchased. </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ProductOrService">
        <xs:annotation>
          <xs:documentation>States whether the line item is for a product or for a service. (see list)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Services"/>
            <xs:enumeration value="Intellectual Property"/>
            <xs:enumeration value="Personal Property"/>
            <xs:enumeration value="Real Property"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="ProductServiceDescription" type="xs:string">
        <xs:annotation>
          <xs:documentation>Description of the product or service.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Quantity" type="DecimalElement5OverZeroType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Quantity ordered.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ItemUIDRequired" type="TrueFalseType">
        <xs:annotation>
          <xs:documentation>Specifies whether or not the item requires UII marking.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PriceBasis" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Type of unit price. (see list)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="Catalog"/>
            <xs:enumeration value="Drained Net Weight"/>
            <xs:enumeration value="Estimated"/>
            <xs:enumeration value="Includes Government Furnished Property (GFP) - Incorporation"/>
            <xs:enumeration value="Includes Government Furnished Property (GFP) - Usage"/>
            <xs:enumeration value="Lump Sum"/>
            <xs:enumeration value="Not to Exceed"/>
            <xs:enumeration value="Not Separately Priced"/>
            <xs:enumeration value="Schedule Price"/>
            <xs:enumeration value="To Be Negotiated"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="UnitOfMeasure" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The unit of measure.  (see Unit of Measure Without Dollars list at dodprocurementtoolbox.com)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UnitPrice" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Price per unit.  Cannot be provided when Price Basis is "Not Separately Priced" or the procurement instrument is a solicitation.</xs:documentation>
        </xs:annotation>
