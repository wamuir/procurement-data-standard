      <xs:pattern value="([0-9A-HJ-NP-Z]){2}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AgencyAccountingIdentifierType">
    <xs:annotation>
      <xs:documentation>Six digit Agency Accounting Identifier</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9]){5}([1-9]){1}"/>
      <xs:pattern value="([0-9]){4}([1-9]){1}([0-9]){1}"/>
      <xs:pattern value="([0-9]){3}([1-9]){1}([0-9]){2}"/>
      <xs:pattern value="([0-9]){2}([1-9]){1}([0-9]){3}"/>
      <xs:pattern value="([0-9]){1}([1-9]){1}([0-9]){4}"/>
      <xs:pattern value="([1-9]){1}([0-9]){5}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AmendmentNumberType">
    <xs:annotation>
      <xs:documentation>Four digit Amendment Number.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9]){4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CAGEType">
    <xs:annotation>
      <xs:documentation>Commercial and Government Entity (CAGE) code.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="5"/>
      <xs:pattern value="([0-9A-Z]){5}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DecimalElement2OverZeroType">
    <xs:annotation>
      <xs:documentation>Format:  Decimal type with 2 fraction digits and greater than zero.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="2"/>
      <xs:minInclusive value="0.01"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DecimalElement2Type">
    <xs:annotation>
      <xs:documentation>Format:  Decimal type with 2 fraction digits</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="2"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DecimalElement5ZeroPlusType">
    <xs:annotation>
      <xs:documentation>Format:  Decimal type with 5 fraction digits and greater than zero.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="5"/>
      <xs:minInclusive value="0"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DecimalElement5OverZeroType">
    <xs:annotation>
      <xs:documentation>Format:  Decimal type with 5 fraction digits and greater than zero.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="5"/>
      <xs:minInclusive value="0.00001"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DecimalElement5Type">
    <xs:annotation>
      <xs:documentation>Format:  Decimal type with 5 fraction digits.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="5"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DecimalElementCoordinateType">
    <xs:annotation>
      <xs:documentation>Format:  Decimal between -180 and 180 with 5 decimal positions</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:fractionDigits value="5"/>
      <xs:minInclusive value="-180"/>
      <xs:maxInclusive value="180"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DITPRType">
    <xs:annotation>
      <xs:documentation>The DoD Information Technology Portfolio Repository (DITPR) Number.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="8"/>
      <xs:pattern value="([0-9])*([1-9]){1}([0-9])*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DoDAACType">
    <xs:annotation>
      <xs:documentation>Department of Defense Activity Address Code (DoDAAC).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="6"/>
      <xs:pattern value="([0-9ACE-HJL-OQRSUVWYZ]){1}([0-9A-Z]){5}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DoDAACType-BadMod">
    <xs:annotation>
      <xs:documentation>Invalid Department of Defense Activity Address Code (DoDAAC).  Used to carry old bad values on Modification/Amendment documents.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="6"/>
      <xs:pattern value="([0-9A-Z]){6}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Duns4NumberType">
    <xs:annotation>
      <xs:documentation>Duns + 4 format.  9 numeric followed by 4 alphanumeric.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9]){9}([0-9A-Z]){4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DunsNumberType">
    <xs:annotation>
      <xs:documentation>Duns format.  9 numeric.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9]){9}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ElinBaseType">
    <xs:annotation>
      <xs:documentation>One or two character Exhibit Line Item Base.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([A-HJ-NP-Z]){1}"/>
      <xs:pattern value="([A-HJ-NP-Z]){2}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ElinNumberType">
    <xs:annotation>
      <xs:documentation>Two or three character Exhibit Line Item Serial Number.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9A-HJ-NP-Z]){2}"/>
      <xs:pattern value="([0-9]){1}([0-9A-HJ-NP-Z]){2}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="GEOLOCType">
    <xs:annotation>
      <xs:documentation>Geographical Location (GEOLOC).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="8"/>
      <xs:pattern value="([0-9A-Z]){1}([0-9A-Z ])*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="GovernmentUniqueEntityIdentifierType">
    <xs:annotation>
      <xs:documentation>Standard format for Government Unique Entity Identifier.  Also known as SAMMI.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([1-9A-Z]){1}([0-9A-Z]){11}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="LineItemBaseType">
    <xs:annotation>
      <xs:documentation>Four digit Line Item Base.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9]){4}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="MAPACType">
    <xs:annotation>
      <xs:documentation>Military Assistance Program Address Code (MAPAC).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="6"/>
      <xs:pattern value="([BDIPKTX]){1}([0-9A-Z]){5}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="NonDoDEntityType">
    <xs:annotation>
      <xs:documentation>CAGE or DUNS</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="5"/>
      <xs:maxLength value="9"/>
      <xs:pattern value="([0-9]){9}"/>
      <xs:pattern value="([0-9A-Z]){5}"/>
      <xs:pattern value="([0-9ACE-HJL-OQRSUVWYZ]){1}([0-9A-Z]){5}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="NonDoDNumberType">
    <xs:annotation>
      <xs:documentation>Four to fifty character non-DoD procurement instrument number.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="4"/>
      <xs:maxLength value="50"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="OrderModificationIdentifierType">
    <xs:annotation>
      <xs:documentation>Two digit Order Modification Identifier.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9A-HJ-NP-Z]){2}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="OrderNumberType">
    <xs:annotation>
      <xs:documentation>Four digit Order Number.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9B-HJ-NQ-Z]){1}([0-9A-HJ-NP-Z]){3}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ProcurementInstrumentModificationIdentifierType">
    <xs:annotation>
      <xs:documentation>Six digit Procurement Instrument Modification Identifier.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([AP]){1}([0-9A-HJ-NP-Z]){2}([0-9]){3}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="SlinExtensionType">
    <xs:annotation>
      <xs:documentation>Two character SLIN Extension.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([A-HJ-NP-Z]){2}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TrueFalseType">
    <xs:annotation>
      <xs:documentation>Format:  Value must be "true" or "false"</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="false"/>
      <xs:enumeration value="true"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TrueType">
    <xs:annotation>
      <xs:documentation>Format:  Value must be "true".</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="true"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="UnitedNationsCodeType">
    <xs:annotation>
      <xs:documentation>United Nations Code for ports and other locations (UN/LOCODE) in accordance with the United Nations Location Code Manual.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([A-Z]){2}([A-Z2-9]){3}"/>
    </xs:restriction>
  </xs:simpleType>
  <!--*** groups-->
  <xs:group name="ConstructionInstrumentLocationGroup">
    <xs:annotation>
      <xs:documentation>Procurement Instrument form locations for construction (CSI) structures.</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="CSIDivision" type="CSIDivisionType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Division of the contract as defined by the Construction Specifications Institute (May 21, 2010).  (see list)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CSISection" type="CSISectionType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Division of the contract used in the Standard Procurement System as defined by the Construction Specifications Institute.  (see list)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:group>
  <xs:group name="ProcurementInstrumentLocationGroup">
    <xs:annotation>
      <xs:documentation>Procurement Instrument form locations for construction (CSI) and UCF structures.</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="CSIDivision" type="CSIDivisionType">
        <xs:annotation>
          <xs:documentation>Division of the contract as defined by the Construction Specifications Institute (April 2012).  (see list)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CSISection" type="CSISectionType">
        <xs:annotation>
          <xs:documentation>Division of the contract used in the Standard Procurement System prior to implementation of the updated Construction Specifications Institute format (April 2012).  (see list)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Section" type="SectionType">
        <xs:annotation>
          <xs:documentation>Section of the Federal Uniform Contract Format (UCF).  (see list)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:group>
  <!--New stuff to be categorized.-->
  <xs:complexType name="UndefinitizedOtherAmountsType">
    <xs:sequence>
      <xs:element name="UndefinitizedIdentifier" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>An identifier that is used to group line items together to relate specific information such as amounts related to a specific undefinitized contract action.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="UndefinitizedOtherAmount" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>Amounts specific to undefinitized contract actions (UCA).</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="UndefinitizedAmountDescription" type="xs:string">
              <xs:annotation>
                <xs:documentation>Type of Amount applicable to an undefinitized contract actions. (see Undefinitized Amount Description list at dodprocurementtoolbox.com)</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="UndefinitizedAmount" type="DecimalElement5ZeroPlusType">
              <xs:annotation>
                <xs:documentation>The amount value.  On an award, must be greater than zero. </xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TargetDefinitizationScheduleType">
    <xs:sequence>
      <xs:element name="UndefinitizedIdentifier" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>An identifier that unqiuely identifies an undefinitized contract action.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ScheduleDates" minOccurs="3" maxOccurs="6">
        <xs:annotation>
          <xs:documentation>Definitization schedule dates.  Must provide the original target date for definitization and dates for the submission of proposal and the beginning of negotiations.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ScheduleDateDescription">
              <xs:annotation>
                <xs:documentation>Description of the schedule date.  (see list)</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:token">
                  <xs:enumeration value="Definitization Target"/>
                  <xs:enumeration value="Proposal Submission"/>
                  <xs:enumeration value="Negotiation Start"/>
                  <xs:enumeration value="Make Or Buy Submission"/>
                  <xs:enumeration value="Subcontracting Plan Submission"/>
                  <xs:enumeration value="Certified Cost Or Pricing Data Submission"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="ScheduleDate" type="DateTimeType">
              <xs:annotation>
                <xs:documentation>The schedule date.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:schema>
