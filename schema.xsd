      <xs:element name="RevisionNumber" type="xs:string">
        <xs:annotation>
          <xs:documentation>The revision number of the wage determination.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CountyCityState" type="CountyCityStateType" maxOccurs="unbounded">
