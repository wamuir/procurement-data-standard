          <xs:documentation>Variation expressed as a discrete quantity.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="QuantityUnder" type="DecimalElement5OverZeroType" minOccurs="0">
