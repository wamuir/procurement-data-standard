                  <xs:enumeration value="FULL DUPLEX [N]"/>
                  <xs:enumeration value="HALF DUPLEX"/>
                  <xs:enumeration value="HALF DUPLEX [N]"/>
                  <xs:enumeration value="HALF DUPLEX R/O"/>
                  <xs:enumeration value="MULTIPOINT S/R"/>
                  <xs:enumeration value="MULTIPOINT S/R [N]"/>
                  <xs:enumeration value="MULTIPOINT R/O"/>
