        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="CDRL" type="CDRLType" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Information about a Contract Data Requirements List (CDRL).</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="LineItems" type="LineItemDetailType" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Information about contract line items.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AwardModificationInstrumentType">
    <xs:annotation>
      <xs:documentation>Award Modification Instrument or Solicitation Amendment Document.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ProcurementInstrumentHeader" type="ProcurementInstrumentHeaderModificationDetails">
        <xs:annotation>
          <xs:documentation>Data common to the entire procurement instrument.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ContractLineItems" minOccurs="0" maxOccurs="2">
        <xs:annotation>
          <xs:documentation>The line item information of the procurement instrument.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:element name="CDRL">
              <xs:annotation>
                <xs:documentation>Information about added, changed, and deleted Contract Data Requirements List (CDRL).</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="AddedCDRLS" type="CDRLType" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Contains CDRLs that were added.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="DeletedCDRLs" type="CDRLType" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Contains CDRLs that were removed.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="ChangedCDRLs" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Contains CDRLs that were modified.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="CDRLChangeText" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Description of the change to the CDRL.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="CurrentCDRL" type="CDRLType">
                          <xs:annotation>
                            <xs:documentation>The CDRL after the change was applied.  The result.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="PreviousCDRL" type="CDRLType">
                          <xs:annotation>
                            <xs:documentation>The CDRL before the change was applied.  The original.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="LineItems">
              <xs:annotation>
                <xs:documentation>Information about added, changed, and deleted line items.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="AddedLineItems" type="LineItemDetailType" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Contains line items that were added.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="DeletedLineItems" type="LineItemDetailType" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Contains line items that were removed.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="ChangedLineItems" minOccurs="0" maxOccurs="unbounded">
                    <xs:annotation>
                      <xs:documentation>Contains line items that were modified.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="LineItemChangeTextDetails" maxOccurs="unbounded">
                          <xs:annotation>
                            <xs:documentation>Descriptive text of the changes made to the line item by UCF section.</xs:documentation>
                          </xs:annotation>
                          <xs:complexType>
                            <xs:sequence>
                              <xs:element name="LineItemChangeText" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>Description of the line item change that applies to the section/division provided in CSI Division, CSI Section, or Section.</xs:documentation>
                                </xs:annotation>
                              </xs:element>
                              <xs:group ref="ProcurementInstrumentLocationGroup"/>
                            </xs:sequence>
                          </xs:complexType>
                        </xs:element>
                        <xs:element name="CurrentLineItem" type="LineItemDetailType">
                          <xs:annotation>
                            <xs:documentation>The line item after the change was applied.  The result.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="PreviousLineItem" type="LineItemDetailType">
                          <xs:annotation>
                            <xs:documentation>The line item before the change was applied.  The original.</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:choice>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
