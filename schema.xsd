                      <xs:documentation>The procurement instrument identifier after the changes were applied.  The results.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                  <xs:element name="PreviousProcurementInstrumentIdentifier" type="ProcurementInstrumentType">
                    <xs:annotation>
                      <xs:documentation>The procurement instrument identifier before the changes were applied.  The original.</xs:documentation>
                    </xs:annotation>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="BasicInformation" type="HeaderBasicInformationModificationType">
        <xs:annotation>
          <xs:documentation>Miscellaneous procurement instrument information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ClauseInformation" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Clause or provision information that applies to the procurement instrument.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="RegulationURL" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>The URL where the text of the clauses or provisions in a Regulation, Supplement, or Issuing Agency can be found.</xs:documentation>
              </xs:annotation>
