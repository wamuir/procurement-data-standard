  <xs:complexType name="USPostalAddressType">
    <xs:annotation>
      <xs:documentation>Mailing address based on US Postal Systems Publication 28</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="NonAddressDataLine" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Contains information that is not required by the US Postal Services for distribution and Delivery.  For example, a mailstop code.  When provided, it is the first line of a printed address.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="AttentionLine" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The person or entity within the Recipient to whom the mail should be directed.  In a printed address, it is placed above the Recipient line.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Recipient">
        <xs:annotation>
          <xs:documentation>Person or entity.  In the printed address, it is placed above the Delivery Address Line or, if provided, the Alternate Location line.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="AlternateLocation" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Provided when Delivery Address Line cannot carry the complete address.  For example, 'Apt C'.  In a printed address, it is placed above Delivery Address Line.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="DeliveryAddressLine">
        <xs:annotation>
          <xs:documentation>The address line.  In a printed address, it is placed above the City/State line.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="PostOfficeBox" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The designator that distinguishes a post office box address.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:choice>
        <xs:element name="City">
          <xs:annotation>
            <xs:documentation>The designator that distinguishes one city from another.</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:maxLength value="40"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="MilitaryOrFederalOverseas">
          <xs:annotation>
            <xs:documentation>The designator that distinguishes one Army/Air Force Post Office (APO), Fleet Post Office (FPO), or Diplomat Post Office (DPO) from another.</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:pattern value="APO"/>
              <xs:pattern value="DPO"/>
              <xs:pattern value="FPO"/>
              <xs:enumeration value="APO"/>
              <xs:enumeration value="DPO"/>
              <xs:enumeration value="FPO"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:choice>
      <xs:element name="StateOrProvince">
        <xs:annotation>
          <xs:documentation>Designates the State, Province, or Territory within a country.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="PostalCode" type="PostalCodeType">
        <xs:annotation>
          <xs:documentation>The designator that distinguishes one postal zone from another.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Country">
        <xs:annotation>
          <xs:documentation>The textual name of the country from Geopolitical Entities, Names, and Codes (GENC).</xs:documentation>
