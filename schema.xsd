                  <xs:enumeration value="6 HOURS OR LESS"/>
                  <xs:enumeration value="6/12 HOURS"/>
                  <xs:enumeration value="CONTINGENCY REQUIREMENT"/>
                  <xs:enumeration value="FREQUENCY SHARED"/>
                  <xs:enumeration value="FULL PERIOD"/>
                  <xs:enumeration value="HOT STANDBY"/>
                  <xs:enumeration value="ON CALL"/>
                  <xs:enumeration value="OTHER"/>
                  <xs:enumeration value="PROGRAMMED PREEMPT"/>
                  <xs:enumeration value="PROGRAMMED REROUTE"/>
                  <xs:enumeration value="SECOND PATH"/>
                  <xs:enumeration value="SECOND PROGRAMMED PREEMPT"/>
                  <xs:enumeration value="SECOND ON CALL"/>
                  <xs:enumeration value="SPECIAL PURPOSE DSCS PATH"/>
                  <xs:enumeration value="SWITCHED NETWORK RESTORAL"/>
                  <xs:enumeration value="TEMPORARY/EXERCISE CIRCUIT"/>
                  <xs:enumeration value="THIRD PROGRAMMED PREEMPT"/>
                  <xs:enumeration value="TIME SHARED"/>
