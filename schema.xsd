                <xs:documentation>Contains procurement instrument identifiers that are added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedProcurementInstrumentIdentifier" type="ProcurementInstrumentType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
                <xs:documentation>Contains procurement instrument identifiers that are removed.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="ChangedProcurementInstrumentIdentifier" minOccurs="0" maxOccurs="unbounded">
