              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:token">
                  <xs:enumeration value="A"/>
                  <xs:enumeration value="B"/>
                  <xs:enumeration value="C"/>
                  <xs:enumeration value="D"/>
                  <xs:enumeration value="E"/>
                  <xs:enumeration value="F"/>
                  <xs:enumeration value="X"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:sequence>
              <xs:element name="GovernmentProvided" type="TrueType">
                <xs:annotation>
                  <xs:documentation>Used when the appropriate statement is not known or it may vary among specification submissions of the data item.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="DistributionStatementText" type="xs:string" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Contains the text of the distribution statement.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:choice>
        </xs:complexType>
      </xs:element>
      <xs:element name="InspectionAcceptance">
        <xs:annotation>
          <xs:documentation>Details about inspection and acceptance.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:choice>
            <xs:sequence>
              <xs:element name="TransmittalLetter" type="TrueType">
