                <xs:documentation>Contains payment discount information that was added.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element name="DeletedPaymentDiscounts" type="PaymentDiscountType" minOccurs="0" maxOccurs="unbounded">
              <xs:annotation>
